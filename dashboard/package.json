{
  "name": "killbug-dashboard",
  "version": "0.0.1",
  "private": true,
  "author": "Francois-Guillaume Ribreau <npm@fgribreau.com> (http://fgribreau.com/)",
  "license": "KillBug",
  "engines": {
    "node": "6"
  },
  "bin": {},
  "scripts": {
    "postinstall": "webpack",
    "build-watch": "webpack --watch",
    "start-dev":"(cd build && python -m SimpleHTTPServer 8084)",
    "test": "mocha $(find src -name '*.test.js')",
    "test-watch": "mocha -w $(find src -name '*.test.js')",
    "test-coverage": "nyc --all --statements=100 --lines=100 --functions=90 --branches=70 --check-coverage --reporter=lcov --reporter=cobertura --report-dir=coverage -- mocha -R spec -t 100000 $(find src -name '*.test.js')",
    "send-coverage": "cat ./coverage/lcov.info | coveralls",
    "update": "updtr",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0",
    "changelog-git": "npm run changelog && git add CHANGELOG.md && git commit -m 'docs(changelog): updated' && git push origin master"
  },
  "main": "index.js",
  "keywords": [],
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "browser-sync": "^2.18.5",
    "browser-sync-webpack-plugin": "^1.1.3",
    "chai": "^3.5.0",
    "conventional-changelog": "^1.1.0",
    "conventional-changelog-cli": "^1.2.0",
    "coveralls": "^2.11.12",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.37.4",
    "flow-typed": "^2.0.0",
    "html-webpack-plugin": "^2.24.1",
    "mocha": "^3.0.1",
    "node-sass": "^4.0.0",
    "nyc": "^8.1.0",
    "postcss-loader": "^1.2.1",
    "sass-loader": "^4.1.0",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "updtr": "^0.2.1",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "auth0-lock": "^10.7.2",
    "bluebird": "^3.4.6",
    "bootstrap": "^4.0.0-alpha.5",
    "classnames": "^2.2.5",
    "codemirror": "^5.21.0",
    "expanding-textareas": "^1.0.2",
    "flat": "^2.0.1",
    "if-exp": "^2.0.2",
    "jquery": "^3.1.1",
    "lodash": "*",
    "material-ui": "^0.16.6",
    "moment": "^2.17.1",
    "mousetrap": "^1.6.0",
    "react": "^15.4.1",
    "react-bootstrap-table": "^2.7.0",
    "react-codemirror": "^0.3.0",
    "react-dom": "^15.4.1",
    "react-json": "^0.2.1",
    "react-json-inspector": "^7.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-toggle-button": "^2.1.0",
    "socket.io-client": "^1.7.2",
    "superagent": "^3.3.0",
    "superagent-use": "^0.1.0",
    "ui-router-react": "^0.3.0",
    "ui-router-visualizer": "^2.0.11"
  }
}
