{
  "name": "killbug-relay",
  "version": "0.0.1",
  "private": true,
  "author": "Francois-Guillaume Ribreau <npm@fgribreau.com> (http://fgribreau.com/)",
  "license": "KillBug",
  "description": "relay",
  "engines": {
    "node": "6"
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "dist",
      "coverage",
      "webpack.config.js",
      "**/**.test.js"
    ]
  },
  "bin": {},
  "scripts": {
    "start": "node server.js",
    "start-dev": "PORT=80 sudo -E nodemon server.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "flow-typed:install": "flow-typed install",
    "start-demo-client": "python -m SimpleHTTPServer 8081",
    "test": "mocha -t 10000 -r ./bootstrap.js $(find src -name '*.test.js')",
    "test-watch": "mocha -t 10000 -r ./bootstrap.js -w $(find src -name '*.test.js')"
  },
  "main": "server.js",
  "devDependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "conventional-changelog": "^1.1.0",
    "conventional-changelog-cli": "^1.2.0",
    "coveralls": "^2.11.12",
    "flow-bin": "^0.37.4",
    "mocha": "^3.0.1",
    "nodemon": "^1.11.0",
    "nyc": "^8.1.0",
    "sinon": "^1.17.5",
    "updtr": "^0.2.1",
    "webpack": "^2.2.0-rc.0"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "common-env": "^6.1.0",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.2.1",
    "lodash": "*",
    "pg": "^6.1.2",
    "requestretry": "^1.12.0",
    "socket.io": "^1.7.2",
    "ua-parser-js": "^0.7.12",
    "uuid": "^3.0.1"
  }
}
